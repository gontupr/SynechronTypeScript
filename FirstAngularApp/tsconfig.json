        "target": "es5",
        "module": "es2015",
        "strict": true,
        "sourceMap": false,
        "noImplicitAny": true,
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "skipLibCheck": true,
